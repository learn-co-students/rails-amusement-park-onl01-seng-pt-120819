<%= form_for @attraction do |f| %>
    <%= f.label "Name" %>
    <%= f.text_field :name%>

    <%= f.label "Tickets" %>
    <%= f.text_area :tickets %>

    <%= f.label "Nausea" %>
    <%= f.text_area :nausea_rating %>

    <%= f.label "Happiness" %>
    <%= f.text_area :happiness_rating %>

    <%= f.label "Height" %>
    <%= f.text_area :min_height %>

    <%= f.submit %>
<% end %>